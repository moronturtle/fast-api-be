{
  "name": "FastAPI Dev Container",
  "workspaceFolder": "/app",
  "dockerComposeFile": "../docker-compose.yml",
  "service": "backend",
  "mounts": [
    "source=${localWorkspaceFolder},target=/app,type=bind"
  ],
  "remoteUser": "root",
  "customizations": {
    "vscode": {
      "extensions": ["ms-python.python", "ms-python.vscode-pylance"]
    }
  }
}
